{
  "name": "CyberChromeCheckout",
  "version": "1.1.0",
  "manifest_version": 2,
  "description": "Activate Cyber Quick Task from Chrome",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
      {
        "run_at": "document_start",
        "matches": [
          "https://*/*"
        ],
        "js": [
          "jquery.js",
          "addCyberButtons.js"
        ],
        "css": [
          "style.css"
        ]
      }
    ]
  }